{
	"name": "jrmajor/bajki-muza",
	"description": "",
	"license": "MIT",
	"type": "project",
	"keywords": [
		"bajki",
		"polskie nagrania muza"
	],
	"require": {
		"php": "~8.3.0",
		"azjezz/psl": "^3.3",
		"guzzlehttp/guzzle": "^7.10",
		"guzzlehttp/psr7": "^2.6",
		"inertiajs/inertia-laravel": "^2.0",
		"intervention/image": "^3.11",
		"jrmajor/laravel-fluent": "^1.2",
		"laravel/framework": "^12.17",
		"laravel/telescope": "^5.11",
		"laravel/tinker": "^2.10",
		"league/flysystem-aws-s3-v3": "^3.29",
		"nesbot/carbon": "^3.4",
		"sentry/sentry-laravel": "^4.15",
		"spatie/laravel-collection-macros": "^8.0",
		"spatie/laravel-schedule-monitor": "^4.1",
		"spatie/laravel-sitemap": "^7.3",
		"spatie/laravel-sluggable": "^3.7",
		"spatie/temporary-directory": "^2.2",
		"symfony/css-selector": "^7.0",
		"symfony/dom-crawler": "^7.3",
		"tightenco/ziggy": "^2.5"
	},
	"require-dev": {
		"barryvdh/laravel-debugbar": "^3.16",
		"barryvdh/laravel-ide-helper": "^3.6",
		"brianium/paratest": "^7.8",
		"fakerphp/faker": "^1.24",
		"jrmajor/cs": "^0.6.1",
		"larastan/larastan": "^3.6",
		"laravel/pail": "^1.2",
		"mockery/mockery": "^1.6",
		"nunomaduro/collision": "^8.7",
		"php-standard-library/phpstan-extension": "^2.0",
		"phpstan/phpstan": "^2.1",
		"phpstan/phpstan-mockery": "^2.0",
		"phpstan/phpstan-phpunit": "^2.0",
		"phpunit/phpunit": "^11.5",
		"spatie/invade": "^2.1",
		"spatie/laravel-ignition": "^2.9",
		"vaimo/composer-patches": "^5.2"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"Database\\Factories\\": "database/factories/",
			"Database\\Seeders\\": "database/seeders/"
		},
		"files": [
			"app/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		},
		"files": [
			"tests/helpers.php"
		]
	},
	"config": {
		"allow-plugins": {
			"vaimo/composer-patches": true
		},
		"sort-packages": true
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		},
		"patcher": {
			"search": "patches"
		}
	},
	"scripts": {
		"dev" : [
			"Composer\\Config::disableProcessTimeout",
			"pnpx concurrently -k -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"pnpm run dev\" --names=server,queue,logs,vite"
		],
		"preview" : [
			"Composer\\Config::disableProcessTimeout",
			"pnpm run build",
			"pnpx concurrently -k -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"pnpm exec php artisan inertia:start-ssr\" --names=server,queue,logs,ssr"
		],
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi",
			"@php artisan vendor:publish --tag=laravel-assets --ansi --force"
		]
	}
}
