{
    "name": "jrmajor/bajki-muza",
    "description": "",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "bajki",
        "polskie nagrania muza"
    ],
    "require": {
        "php": "~8.1.0",
        "azjezz/psl": "2.0.0-rc2",
        "doctrine/dbal": "^3.3",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.4",
        "jrmajor/laravel-fluent": "^0.1.5",
        "larabug/larabug": "^2.5",
        "laravel/framework": "^9.5",
        "laravel/telescope": "^4.8",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.10",
        "nesbot/carbon": "^2.57",
        "nunomaduro/curryable": "master",
        "ohdearapp/ohdear-php-sdk": "^3.4",
        "spatie/data-transfer-object": "^3.7",
        "spatie/image": "^2.2",
        "spatie/laravel-collection-macros": "^7.11",
        "spatie/laravel-schedule-monitor": "^3.0",
        "spatie/laravel-sluggable": "^3.3",
        "spatie/temporary-directory": "^2.0",
        "symfony/css-selector": "^6.0",
        "symfony/dom-crawler": "^6.0",
        "tightenco/ziggy": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.19",
        "jrmajor/cs": "^0.3.4",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.1",
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "10.0.x-dev",
        "spatie/invade": "^1.0",
        "spatie/laravel-ignition": "^1.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/helpers.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
        "post-create-project-cmd": "@php artisan key:generate --ansi"
    }
}
