{
    "name": "jrmajor/bajki-muza",
    "description": "",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "bajki",
        "polskie nagrania muza"
    ],
    "require": {
        "php": "~8.1.0",
        "azjezz/psl": "^2.4",
        "doctrine/dbal": "^3.6",
        "guzzlehttp/guzzle": "^7.5",
        "jrmajor/laravel-fluent": "^0.2.1",
        "larabug/larabug": "^2.5",
        "laravel/framework": "^9.52",
        "laravel/telescope": "^4.14",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.12",
        "livewire/livewire": "^2.12",
        "nesbot/carbon": "^2.66",
        "nunomaduro/curryable": "master",
        "ohdearapp/ohdear-php-sdk": "^3.5",
        "spatie/image": "^2.2",
        "spatie/laravel-collection-macros": "^7.12",
        "spatie/laravel-schedule-monitor": "^3.2",
        "spatie/laravel-sluggable": "^3.4",
        "spatie/temporary-directory": "^2.1",
        "symfony/css-selector": "^6.2",
        "symfony/dom-crawler": "^6.2",
        "tightenco/ziggy": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.21",
        "jrmajor/cs": "^0.5.0",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.4",
        "nunomaduro/larastan": "^2.4",
        "php-standard-library/phpstan-extension": "^1.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-mockery": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^10.0",
        "spatie/invade": "^1.1",
        "spatie/laravel-ignition": "^1.6",
        "vaimo/composer-patches": "^5.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/helpers.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "vaimo/composer-patches": true
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "patcher": {
            "search": "patches"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    }
}
