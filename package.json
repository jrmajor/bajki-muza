{
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.8.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && vite build --ssr",
    "check": "php artisan ziggy:generate --types-only && svelte-check --tsconfig ./tsconfig.json --fail-on-warnings",
    "check:watch": "php artisan ziggy:generate --types-only && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . --max-warnings 0",
    "show-bundle": "vite-bundle-visualizer"
  },
  "dependencies": {
    "@inertiajs/svelte": "^2.0",
    "@tailwindcss/forms": "^0.5.10",
    "esm-env": "^1.2",
    "pretty-bytes": "^7.0",
    "svelte": "^5.33",
    "tailwindcss": "^4.1"
  },
  "devDependencies": {
    "@jrmajor/eslint-config": "^0.1.1",
    "@sveltejs/vite-plugin-svelte": "^5.1",
    "@tailwindcss/vite": "^4.1",
    "@types/node": "^22.15",
    "eslint": "^9.28",
    "globals": "^16.2",
    "laravel-vite-plugin": "^1.3",
    "svelte-check": "^4.2",
    "typescript": "^5.8",
    "vite": "^6.3",
    "vite-bundle-visualizer": "^1.2"
  },
  "volta": {
    "node": "22.11.0",
    "pnpm": "10.8.0"
  }
}
